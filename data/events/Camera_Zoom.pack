public var camTween:FlxTween;
function onEvent(_) {
    if (_.event.name == 'Camera_Zoom') {
		if (camTween != null) {
			camTween.cancel(); camTween = null;
		}
		var val1 = Std.parseFloat(_.event.params[0]);
		if(Math.isNaN(val1)) val1 = 1;

		var targetZoom = defaultCamZoom * val1;
		if (_.event.params[1] != '') {
			var duration = 0;
			if(_.event.params[1] != null) duration = Std.parseFloat(_.event.params[1]);
			if(Math.isNaN(duration)) duration = 0;

			if (duration > 0) {
				camTween = FlxTween.tween(FlxG.camera, {zoom: targetZoom}, duration, {ease: FlxEase.circOut});
			}
			else FlxG.camera.zoom = targetZoom;
		}
		defaultCamZoom = targetZoom;
	}
}________PACKSEP________{
	"params": [
		{
			"name": "Zoom",
			"type": "String",
			"defaultValue": "1"
		}, {
			"name": "Time",
			"type": "String",
			"defaultValue": "0"
		}
	]
}